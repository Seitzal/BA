  publisher = {{VS Verlag für Sozialwissenschaften}},
  location = {{Wiesbaden}},
  abstract = {Die Funktion der Massenmedien im politischen Meinungs- und Willensbil-dungsprozeß, insbesondere bei Wahlen, wird seit jeher heftig und in der Regel kontrovers diskutiert. In der mehr als 50-jährigen Geschichte der empirischen Medienwirkungsforschung wechselten sich Phasen, in denen die Massenmedien als wirkungsmächtig angesehen wurden, und Phasen, in denen ihnen — wenn überhaupt — nur geringe Effekte zugeschrieben wurden, ab. In der Bundesrepublik lösten vor allem Untersuchungen von Noelle-Neumann (1977) sowie von Kepplinger (1980) im Zuge der Bundestagswahl 1976 eine politische Kontroverse über den Einfluß des Fernsehens auf das Wahlverhalten aus (Schulz/Schönbach 1983; Kaase 1989a).},
  isbn = {978-3-322-95869-3},
  langid = {german},
  series = {Beiträge zu den Berichten der Kommission für die Erforschung des sozialen und politischen Wandels in den neuen Bundesländern e.V. (KSPW)}
}

@incollection{schumann1997,
  title = {Formen und Determinanten der Protestwahl},
  booktitle = {Politische Orientierungen und Verhaltensweisen im vereinigten Deutschland},
  author = {Schumann, Siegfried},
  editor = {Gabriel, Oscar W.},
  date = {1997},
  pages = {401--421},
  publisher = {{VS Verlag für Sozialwissenschaften}},
  location = {{Wiesbaden}},
  abstract = {Unabhängig von der Vereinigungsproblematik wurde in den letzten Jahren verstärkt das Thema „Protestwahl“ — einschließlich der Wahlenthaltung aus Protest — diskutiert. Der Hintergrund: Allem Anschein nach war in der Bundesrepublik eine spürbare Zunahme der Neigung zur Protestwahl zu verzeichnen. Die besondere Relevanz dieses Themas für die Neuen Bundesländer liegt auf der Hand. Hier wurde ein neues politisches System installiert, mit für den einzelnen oft gravierenden Konsequenzen hinsichtlich seiner Lebensumstände. Die Stabilität dieses Systems dürfte zumindest auf die Dauer nicht unwesentlich davon abhängen, in welchem Maße es Unterstützung durch die Bevölkerung erfährt. Nach allen derzeit vorliegenden Umfragedaten ist in den Neuen Bundesländern allerdings eher eine Tendenz zur Unzufriedenheit mit dem neuen System und eine entsprechende Protesthaltung feststellbar. Mögliche Gründe für einen Protest gibt es aus individueller Sicht genug. Ist es nicht ein Grund zu protestieren, wenn statt der erhofften „blühenden Landschaften“ erst einmal wirtschaftliche Problemgebiete mit entsprechenden Folgeerscheinungen wie Arbeitslosigkeit, sozialem Abstieg etc. entstehen oder wenn Politiker ganz offensichtlich ihre Versprechen nicht halten? Wenn ja, dann lautet eine wichtige Frage: Wogegen richtet sich der Protest? Gegen einzelne Parteien, deren Politiker in den Augen des Wählers schuld an den Mißständen sind, oder gegen alle Parteien überhaupt? Letzteres würde vermutlich langfristig zur Destabilisierung des politischen Systems beitragen.},
  isbn = {978-3-322-95869-3},
  langid = {german},
  series = {Beiträge zu den Berichten der Kommission für die Erforschung des sozialen und politischen Wandels in den neuen Bundesländern e.V. (KSPW)}
}

