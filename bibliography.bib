}

@report{manow2005,
  title = {National {{Vote Intention}} and {{European Voting Behavior}}, 1979-2004: {{Second Order Election Effects}}, {{Election Timing}}, {{Government Approval}} and the {{Europeanization}} of {{European Elections}}},
  shorttitle = {National {{Vote Intention}} and {{European Voting Behavior}}, 1979-2004},
  author = {Manow, Philip},
  date = {2005},
