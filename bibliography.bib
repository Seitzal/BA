@incollection{mahmutovic2019,
  title = {The {{Democratic Deficit}} of the {{EU}}: {{Two Schools Under One Roof}}},
  shorttitle = {The {{Democratic Deficit}} of the {{EU}}},
  booktitle = {Balkan {{Yearbook}} of {{European}} and {{International Law}} 2019},
